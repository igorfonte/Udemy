Algoritmo "consumo"

Var
   distancia : inteiro
   combustivel, consumo : real


Inicio
    escreva("Distância percorrida: ")
    leia(distancia)
    escreva("Combustivel gasto: ")
    leia(combustivel)
    
    consumo <- distancia / combustivel
    escreval("Consumo médio = ", consumo:8:3)


Fimalgoritmo